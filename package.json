{
  "license": "CC0-1.0",
  "devDependencies": {
    "@types/p5": "^0.9.1",
    "@typescript-eslint/eslint-plugin": "4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "eslint": "7.18.0",
    "eslint-config-prettier": "^7.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rollup": "^2.36.1",
    "terser": "^5.5.1",
    "typescript": "^4.1.3"
  },
  "private": true,
  "scripts": {
    "build": "run-s transpile bundle lint:dist format:dist minify",
    "transpile": "tsc --skipLibCheck",
    "bundle": "rollup --config",
    "lint:dist": "eslint --fix dist/script.js",
    "format:dist": "prettier --write dist/script.js",
    "minify": "terser dist/script.js --output dist/script.min.js --compress --mangle"
  }
}
